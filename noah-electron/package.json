{
  "name": "Noah",
  "author": "Ark Foundation Intern Team",
  "version": "0.1.4",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.1",
    "react-scripts": "5.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
  },
  "main": "main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder build --win -c.extraMetadata.main=main.js --publish never",
    "publish": "electron-builder build --win -c.extraMetadata.main=main.js --publish always",
    "electron-react": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4"
  },
  "proxy": "http://localhost:3000",
  "build": {
    "extraMetadata": {
    "main": "dist/main.js"
    },
  "extends": null,
  "files": [
    "dist/**/*",
    "package.json",
    "public/**/*",
    "src/**/*",
    "main.js"
  ],
    "publish": {
    "provider": "github",
    "owner": "eli-johnson-04",
    "repo": "noah",
    "private": true,
    "token": "${env.GITHUB_ACCESS_TOKEN}"
    }
  }
}
